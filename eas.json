{
  "cli": {
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "android": {
        "buildType": "apk",
        "env": {
          "EAS_BUILD_POST_INSTALL": "npm install --legacy-peer-deps && npx patch-package"
        },
        "channel": "preview"
      }
    },
    "production": {
      "android": {
        "buildType": "app-bundle",
        "channel": "production"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
